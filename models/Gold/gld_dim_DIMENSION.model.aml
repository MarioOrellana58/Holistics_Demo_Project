Model gld_dim_users {
  type: 'query'
  label: 'Gld Dim Users'
  description: ''
  data_source_name: 'demodb'
  dimension email_norm {
    label: 'Email Norm'
    type: 'text'
    hidden: false
    definition: @sql {{ #SOURCE.email_norm }};;
  }
  dimension email_rank {
    label: 'Email Rank'
    type: 'number'
    hidden: false
    definition: @sql {{ #SOURCE.email_rank }};;
  }
  dimension id {
    label: 'Id'
    type: 'number'
    hidden: false
    definition: @sql {{ #SOURCE.id }};;
  }
  dimension sign_up_at {
    label: 'Sign Up At'
    type: 'datetime'
    hidden: false
    definition: @sql {{ #SOURCE.sign_up_at }};;
  }
  dimension first_name {
    label: 'First Name'
    type: 'text'
    hidden: false
    definition: @sql {{ #SOURCE.first_name }};;
  }
  dimension last_name {
    label: 'Last Name'
    type: 'text'
    hidden: false
    definition: @sql {{ #SOURCE.last_name }};;
  }
  dimension birth_date {
    label: 'Birth Date'
    type: 'date'
    hidden: false
    definition: @sql {{ #SOURCE.birth_date }};;
  }
  dimension gender {
    label: 'Gender'
    type: 'text'
    hidden: false
    definition: @sql {{ #SOURCE.gender }};;
  }
  dimension city_id {
    label: 'City Id'
    type: 'number'
    hidden: false
    definition: @sql {{ #SOURCE.city_id }};;
  }
  dimension full_name {
    label: 'Full Name'
    type: 'text'
    hidden: false
    definition: @sql {{ #SOURCE.full_name }};;
  }

  owner: 'quinn+001@holistics.io'
  query: @sql
    SELECT
      email_norm,
      email_rank,
      id,
      sign_up_at,
      first_name,
      last_name,
      birth_date,
      gender,
      city_id,
      full_name
    FROM {{ #slv_dim_users }};;
  models: [
    slv_dim_users
  ]
}
